
rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    match /pacientes/{document=**} {
      allow read, write: if true;
    }
    match /medicos/{document=**} {
      allow read, write: if true;
    }
    match /enfermeiros/{document=**} {
      allow read, write: if true;
    }
    match /departamentos/{document=**} {
      allow read, write: if true;
    }
    match /filaDeEspera/{document=**} {
      allow read, write: if true;
    }
    match /chamadas/{document=**} {
      allow read, write: if true;
    }
    match /counters/{document=**} {
      allow read, write: if true;
    }
    match /relatorios_atendimentos/{document=**} {
      allow read, write: if true;
    }
  }
}
